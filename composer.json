{
  "name": "anibalsanchez/extly-html-asset-tags-builder",
  "description": "The Asset Tags Builder manages the generation of script and style tags for an Html Document.",
  "license": "MIT",
  "version": "2.0.0",
  "authors": [
    {
      "name": "Andrea Gentil & Anibal Sanchez, Extly CB",
      "email": "team@extly.com"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "illuminate/collections": "^8.3.4 || ^9.1.5"
  },
  "require-dev": {
    "phan/phan": "5.x",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Extly\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "config": {
      "platform": {
        "php": "8.0.25"
      }
    }
  }
}
